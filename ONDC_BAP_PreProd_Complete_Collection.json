    "version": {
      "major": 2,
      "minor": 0,
      "patch": 0
    }
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://neo-server.rozana.in",
      "type": "string",
      "description": "Production BAP URL"
    },
    {
      "key": "local_url",
      "value": "http://localhost:8000",
      "type": "string",
      "description": "Local development BAP URL"
    },
    {
      "key": "subscriber_id",
      "value": "neo-server.rozana.in",
      "type": "string",
      "description": "ONDC Subscriber ID"
    },
    {
      "key": "domain",
      "value": "ONDC:RET10",
      "type": "string",
      "description": "ONDC Domain (Retail)"
    },
    {
      "key": "legacy_domain",
      "value": "nic2004:52110",
      "type": "string",
      "description": "Legacy ONDC Domain Code"
    },
    {
      "key": "preprod_registry",
      "value": "https://preprod.registry.ondc.org",
      "type": "string",
      "description": "Pre-production ONDC Registry"
    },
    {
      "key": "staging_registry",
      "value": "https://staging.registry.ondc.org",
      "type": "string",
      "description": "Staging ONDC Registry"
    },
    {
      "key": "pramaan_url",
      "value": "https://pramaan.ondc.org",
      "type": "string",
      "description": "Pramaan eKYC Service URL"
    },
    {
      "key": "transaction_id",
      "value": "",
      "type": "string",
      "description": "Current transaction ID - will be auto-generated"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{auth_token}}",
        "type": "string"
      }
    ]
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "exec": [
          "// Generate UUID v4 for transaction_id if not already set",
          "if (!pm.collectionVariables.get('transaction_id') || pm.collectionVariables.get('transaction_id') === '') {",
          "    const uuid = require('uuid');",
          "    const transactionId = uuid.v4();",
          "    pm.collectionVariables.set('transaction_id', transactionId);",
          "    console.log('Generated transaction_id:', transactionId);",
          "}"
        ],
        "type": "text/javascript"
      }
    }
  ],
